# Application Environment
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# Authentication
AUTH_SECRET=a-random-secret-key-for-authentication

# QWEN AI Configuration - Update these with your actual API key and settings
QWEN_API_KEY=your-qwen-api-key-goes-here
QWEN_API_URL=https://api.qwen.ai/v1
QWEN_MODEL=qwen-max
QWEN_EMBEDDING_MODEL=text-embedding-ada-002

# API Server Configuration
API_PORT=8081

# Vite Configuration
VITE_PUBLIC_APP_URL=http://localhost:8080

#--------------------------------------------------------------------
# QWEN AI Configuration
#--------------------------------------------------------------------

#--------------------------------------
# SERVER-SIDE VARIABLES (Node.js only)
#--------------------------------------
# These variables are only accessible in server-side code (like API routes)
# They will NOT be bundled with your client-side code

#--------------------------------------
# CLIENT-SIDE VARIABLES (Browser)
#--------------------------------------
# For development, we're using the Vite proxy for API requests
# No need to expose the API key on the client in development mode
# The proxy configuration in vite.config.ts will handle the API key

VITE_QWEN_API_URL=https://api.qwen.ai/v1
VITE_QWEN_MODEL=qwen2.5-7b-instruct
VITE_QWEN_EMBEDDING_MODEL=qwen2.5-embedding 